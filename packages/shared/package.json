{
  "name": "@logmydose/shared",
  "version": "1.0.0",
  "private": true,
  "description": "Shared types, interfaces, DTOs, and Prisma client for LogMyDose platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "import": "./dist/entities/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/interfaces/index.js"
    },
    "./dto": {
      "types": "./dist/dto/index.d.ts",
      "import": "./dist/dto/index.js"
    },
    "./prisma": {
      "types": "./dist/prisma/index.d.ts",
      "import": "./dist/prisma/index.js"
    },
    "./queues": {
      "types": "./dist/queues/index.d.ts",
      "import": "./dist/queues/index.js"
    }
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "prisma": "^7.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
